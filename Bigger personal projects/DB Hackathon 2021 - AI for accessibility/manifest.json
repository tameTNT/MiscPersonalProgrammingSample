{
    "name": "Getting Started Example",
    "version": "1.0",
    "description": "Building a Chrome Extension!",
    "manifest_version": 3,
    "permissions": [
        "storage",
        "activeTab",
        "scripting",
        "notifications"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "options_page": "options.html",
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "imgs/get_started16.png",
            "32": "imgs/get_started32.png",
            "48": "imgs/get_started48.png",
            "128": "imgs/get_started128.png"
        }
    },
    "icons": {
        "16": "imgs/get_started16.png",
        "32": "imgs/get_started32.png",
        "48": "imgs/get_started48.png",
        "128": "imgs/get_started128.png"
    }
}